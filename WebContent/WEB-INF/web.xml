<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>MrFaulty</display-name>
  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>uk.ac.dundee.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>insert</servlet-name>
    <servlet-class>uk.ac.dundee.InsertFault</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Adm_Dev_main</servlet-name>
    <servlet-class>uk.ac.dundee.Adm_Dev_main</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>test</servlet-name>
    <servlet-class>uk.ac.dundee.testservlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>search</servlet-name>
    <servlet-class>uk.ac.dundee.SearchServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>delete</servlet-name>
    <servlet-class>uk.ac.dundee.DeleteServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>adduser</servlet-name>
    <servlet-class>uk.ac.dundee.AddUser</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>update</servlet-name>
    <servlet-class>uk.ac.dundee.UpdateFault</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>deleteuserservlet</servlet-name>
    <servlet-class>uk.ac.dundee.DeleteUserServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>assignfaultservlet</servlet-name>
    <servlet-class>uk.ac.dundee.AssignFaultServlet</servlet-class>
  </servlet>  
  

  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>insert</servlet-name>
    <url-pattern>/insert</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Adm_Dev_main</servlet-name>
    <url-pattern>/admdev/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/testservlet/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>search</servlet-name>
    <url-pattern>/search/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>delete</servlet-name>
    <url-pattern>/delete/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>adduser</servlet-name>
    <url-pattern>/adduser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>update</servlet-name>
    <url-pattern>/update</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>deleteuserservlet</servlet-name>
    <url-pattern>/deleteuserservlet/*</url-pattern>
  </servlet-mapping>
    <servlet-mapping>
    <servlet-name>assignfaultservlet</servlet-name>
    <url-pattern>/assignfaultservlet/*</url-pattern>
  </servlet-mapping>
</web-app>